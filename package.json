{
  "name": "checklist",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-dev": "cross-env ENVFILE=.env.dev node node_modules/react-native/local-cli/cli.js run-android --variant=devDebug --appIdSuffix dev",
    "clean-android-dev": "cd android && cross-env ENVFILE=.env.dev ./gradlew clean && cd .. && npm run android-dev",
    "build-android-stage-apk": "cd android && cross-env ENVFILE=.env.stage node ../node_modules/.bin/env-cmd -f .env.android ./gradlew assembleStageRelease",
    "build-android-prod-apk": "cd android && cross-env ENVFILE=.env.prod node ../node_modules/.bin/env-cmd -f .env.android ./gradlew assembleProdRelease",
    "build-android-stage": "cd android && cross-env ENVFILE=.env.stage node ../node_modules/.bin/env-cmd -f .env.android ./gradlew bundleStageRelease",
    "build-android-prod": "cd android && cross-env ENVFILE=.env.prod node ../node_modules/.bin/env-cmd -f .env.android ./gradlew bundleProdRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
